{
  "name": "github-api",
  "version": "1.0.0",
  "description": "An api that consumes github rest api and display repo name, repo description and number of stars for an owner/organization",
  "main": "server.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "node build/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
    "lint": "eslint --ext .ts,.js src/**",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.+(ts|js|json)\" --write",
    "test": "jest"
  },
  "keywords": [
    "github",
    "rest-api"
  ],
  "author": "Michael S. Olawuni",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3",
    "yup": "^0.32.11"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^4.3.0",
    "@octokit/rest": "^19.0.4",
    "compression": "^1.7.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "morgan": "^1.10.0",
    "redis": "^4.3.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  }
}
